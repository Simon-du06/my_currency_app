{
  "expo": {
    "name": "currency_app",
    "slug": "currency_app",
    "description": "A good looking mobile app that allow to convert Euro to multiple currency",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mycurrencyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreeImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Lato-Bold.ttf",
            "./assets/fonts/Lato-SemiBold.ttf",
            "./assets/fonts/Lato-ExtraBold.ttf",
            "./assets/fonts/Lato-Black.ttf",
            "./assets/fonts/Lato-Light.ttf",
            "./assets/fonts/Lato-ExtraLight.ttf",
            "./assets/fonts/Lato-Regular.ttf",
            "./assets/fonts/Lato-Thin.ttf",
            "./assets/fonts/Lato-Medium.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "237cd178-46d9-4ece-a102-c62851767a15"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/237cd178-46d9-4ece-a102-c62851767a15"
    }
  }
}
